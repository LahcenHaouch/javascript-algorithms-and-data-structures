{
  "extends": ["gristow"],
  "rules": {
    "no-console": 1,
    "semi": 0,
    "@typescript-eslint/semi": [0],
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "es5",
        "singleQuote": true,
        "printWidth": 120,
        "tabWidth": 2,
        "semi": false
      }
    ]
  }
}
